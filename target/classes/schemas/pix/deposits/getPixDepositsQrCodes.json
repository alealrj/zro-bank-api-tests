{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "data": {
      "type": "object",
      "properties": {
        "page": {
          "type": "integer"
        },
        "page_size": {
          "type": "integer"
        },
        "page_total": {
          "type": "integer"
        },
        "total": {
          "type": "integer"
        },
        "data": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "key_id": {
                  "type": "string"
                },
                "emv": {
                  "type": "string"
                },
                "txid": {
                  "type": "string"
                },
                "value": {
                  "type": ["null", "integer", "number"]
                },
                "summary": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "state": {
                  "type": "string"
                },
                "created_at": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "key_id",
                "emv",
                "txid",
                "value",
                "summary",
                "description",
                "state",
                "created_at"
              ]
            }
          ]
        }
      },
      "required": [
        "page",
        "page_size",
        "page_total",
        "total",
        "data"
      ]
    },
    "error": {
      "type": "null"
    }
  },
  "required": [
    "success",
    "data",
    "error"
  ]
}